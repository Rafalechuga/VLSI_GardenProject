LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL;
--sele uM0, dM0, uH0, dH0, uM1, dM1, uH1, dH1, alarm
ENTITY selSs7 IS
PORT(
	ss70, ss71: STD_LOGIC_VECTOR( 6 DOWNTO 0 );
	sel: INTEGER RANGE 0 TO 3;
	ss7Out: STD_LOGIC_VECTOR( 6 DOWNTO 0 )
);
END ENTITY;

ARCHITECTURE arq OF alarm IS
BEGIN 
	WITH sel SELECT ss7Out <=
	ss70 WHEN 0,
	ss70 WHEN 1,
	ss70 WHEN 2,
	ss71 WHEN 3,
	ss71 WHEN OTHERS;
	
		
		
END ARCHITECTURE;